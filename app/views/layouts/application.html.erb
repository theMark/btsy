<!DOCTYPE html>
<html>
<head>
  <title>Hembee</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="GlobalNav">
  <div class="GlobalNav-browse">
    <div class="GlobalNav-layout wrapper">
      <div class="GlobalNav-leftSide">
        <!-- <a href="" id="lockup"></a> -->
        <%= link_to "", root_url, :id => "lockup" %>
        <div class="nav-tabs">
          <%= link_to "Shop Hembee", root_url %>
        </div>
      </div>
    </div>
  </div>
</div>

<div class="GlobalNav-spacer"></div>
<div class="wrapper">
  <%= yield %>
</div>

</body>
</html>
